pipeline {
   agent any

    stages {
        stage('Clone sources') {
            steps {
               git 'https://github.com/BibhutiCh/mvnwebapp.git'
            }
        }
        stage('SonarQube analysis') {
            steps{
                withSonarQubeEnv(credentialsId: '0e6e509f9d8074d2b0335eac2b5ab5971837f8d0', installationName: 'SonarQubeServer'){
                sh 'mvn org.sonarsource.scanner.maven:SonarQubeScanner-9.3:sonar'
                }
            }
        }
    }
}
