pipeline {
   agent any

    stages {
        stage('Clone sources') {
            steps {
               git 'https://github.com/BibhutiCh/mvnwebapp.git'
            }
        }
        stage('SonarQube analysis') {
            def scannerHome = tool 'SonarScanner 4.7.0.2747';
            steps{
                withSonarQubeEnv('SonarQube'){
                bat "mvn sonar:sonar"
                }
            }
        }
    }
}
